@model Presentation.Models.ResourceVM

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>ResourceVM</h4>
    <hr />
    @foreach (var res in ViewBag.result) {
    <dl class="dl-horizontal">
        <dt>
            NAME
        </dt>
        <dd>
            @res.name
        </dd>
        <dt>
            ID
        </dt>
        <dd>
            @res.userId
        </dd>
    </dl>
    
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = @res.userId }) |
    @Html.ActionLink("Back to List", "Index")
</p>
}
